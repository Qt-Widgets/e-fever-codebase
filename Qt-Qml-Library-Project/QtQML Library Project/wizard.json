{
    "version": 1,
    "kind": "project",
    "id": "QtQmProjectlLibrary.12a7c9",
    "category": "EFCodeBase",
    "trDescription": "Creates a library project with Testable",
    "trDisplayName": "Qt/QML Library Project",
    "trDisplayCategory": "E-Fever Codebase",
    "enabled": "%{JS: [ %{Plugins} ].indexOf('QmlProjectManager') >= 0}",
    "options": [
        {
            "key": "ProFileName",
            "value": "%{JS: Util.fileName('tests/%{ProjectName}unittests/%{ProjectName}unittests', 'pro')}"
        },
        {
            "key": "MainQmlFileName",
            "value": "%{JS: Util.fileName('%{ProjectName}', 'qml')}"
        },
        {
            "key": "QtQuickVersion",
            "value": "%{JS: %{QtVersion}.qtQuickVersion}"
        },
        {
            "key": "QtQuickWindowVersion",
            "value": "%{JS: %{QtVersion}.qtQuickWindowVersion}"
        },
        {
            "key": "UiSupport",
            "value": "%{JS: '%{QtQuickVersion}' !== '2.3' }"
        },
        {
            "key": "IsUiFileInUse",
            "value": "%{JS: %{UiSupport} && %{QmlUiSplit} }"
        },
        {
            "key": "TargetPathBaseName",
            "value": "%{JS: '%{TargetPath}'.split('/').slice(-1)[0]}"
        }
    ],
    "pages": [
        {
            "trDisplayName": "Project Location",
            "trShortTitle": "Location",
            "typeId": "Project"
        },
        {
            "trDisplayName": "Project Information",
            "trShortTile": "Package",
            "typeId": "Fields",
            "data": [
                {
                    "name": "Package",
                    "trDisplayName": "QML Package Name",
                    "mandatory": true,
                    "type": "LineEdit",
                    "data": {
                        "trText": "%{JS: '%{TargetPathBaseName}'.charAt(0).toUpperCase() + '%{TargetPathBaseName}'.slice(1)}",
                        "validator": "[a-zA-Z][a-zA-Z0-9]*"
                    }
                }
            ]
        },
        {
            "trDisplayName": "Project Management",
            "trShortTitle": "Summary",
            "typeId": "Summary"
        }
    ],
    "generators": [
        {
            "typeId": "File",
            "data": [
                {
                    "source": ".gitignore",
                    "target": ".gitignore"
                },
                {
                    "source": ".travis.yml",
                    "target": ".travis.yml"
                },
                {
                    "source": "appveyor.yml",
                    "target": "appveyor.yml"
                },
                {
                    "source": "LICENSE",
                    "target": "LICENSE"
                },
                {
                    "source": "MYPACKAGE/dummy.qml",
                    "target": "%{Package}/dummy.qml"
                },
                {
                    "source": "MYPROJECT.pri",
                    "target": "%{ProjectName}.pri"
                },
                {
                    "source": "MYPROJECT.pro",
                    "target": "%{ProjectName}.pro",
                    "openAsProject": true
                },
                {
                    "source": "MYPROJECT.qrc",
                    "target": "%{ProjectName}.qrc"
                },
                {
                    "source": "README.md",
                    "target": "README.md"
                },
                {
                    "source": "tests/unittests/.gitignore",
                    "target": "tests/unittests/.gitignore"
                },
                {
                    "source": "tests/unittests/main.cpp",
                    "target": "tests/unittests/main.cpp"
                },
                {
                    "source": "tests/unittests/qmltests/tst_QmlTests.qml",
                    "target": "tests/unittests/qmltests/tst_QmlTests.qml"
                },
                {
                    "source": "tests/unittests/qpm.json",
                    "target": "tests/unittests/qpm.json"
                },
                {
                    "source": "tests/unittests/qpm.pri",
                    "target": "tests/unittests/qpm.pri"
                },
                {
                    "source": "tests/unittests/tests.cpp",
                    "target": "tests/unittests/tests.cpp"
                },
                {
                    "source": "tests/unittests/tests.h",
                    "target": "tests/unittests/tests.h"
                },
                {
                    "source": "tests/unittests/unittests.pro",
                    "target": "tests/unittests/unittests.pro"
                },
                {
                    "source": "tests/unittests/XBacktrace.h",
                    "target": "tests/unittests/XBacktrace.h"
                }
            ]
        }
    ]
}