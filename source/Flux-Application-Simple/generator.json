{
    "ignoreFilePattern": [
        "\\.swp$",
        "\\.DS_Store$",
        "\\.pro.user$",
        "\\.qmlc$",
        "\\.jsc$"
    ],
    "rules": [
        {
            "pattern": ".*",
            "content": [
                {
                    "find": "\\\\",
                    "replace": "\\\\"
                }
            ]
        },
        {
            "pattern": "\\.pro$",
            "openAsProject": true
        }
    ]
}